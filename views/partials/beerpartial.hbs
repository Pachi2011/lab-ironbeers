<div class="col-md-4">
  <div class="beer">
    <img class="beer-img-top" src="{{image_url}}" alt="{{this.name}}"/>
    <div class="beer-body">
      <h5 class="beer-name">{{this.name}}</h5>
      <p class="beer-tagline">{{this.tagline}}</p>
      <p class="beer-description">{{this.description}}</p>
    </div>
  </div>
</div>
